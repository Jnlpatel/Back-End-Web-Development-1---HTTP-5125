@model IEnumerable<Cumulative1.Models.Teacher>


<!-- Displaying all the teachers (first name and last name) on the webpage as link where you can select any teacher for their further details -->
@foreach (Cumulative1.Models.Teacher Teacher in Model)
{
    <div>
        <a href="/TeacherPage/Show/@Teacher.TeacherId">@Teacher.TeacherFName @Teacher.TeacherLName</a>
    </div>
}